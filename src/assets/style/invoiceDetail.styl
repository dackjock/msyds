@import './base'
page
  background: #eee
.container
  background: #eee
.dtop
  height 90rpx
  line-height 90rpx
  background-color: #ffffff
  text-align: center
  margin-bottom: 10rpx
.dtli
  height 90rpx
  border-bottom: 2rpx solid #eeeeee
  padding: 0 30rpx
  color: #666666
  display: flex
  align-items: center
  justify-content: space-between
  .dtbtna
    width 160rpx
    height 65rpx
    line-height: 65rpx
    text-align: center
    color: #ffffff
    background-color: $theme-color
    border-radius: 10rpx
    display: inline-block
    margin-left: 10rpx
.dtbtnbx
  padding: 10rpx 30rpx
  .dtbtnb
    width 100%
    height 80rpx
    line-height: 80rpx
    text-align: center
    color: #ffffff
    background-color: $theme-color
    border-radius: 10rpx
    font-size: 30rpx
.c9
  color: #999999
.red
  color: $theme-color
.fs36
  font-size: 36rpx
.fs24
  font-size: 24rpx
.bgf
  background-color: #ffffff