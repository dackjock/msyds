page
  padding-bottom: 100rpx
  box-sizing: border-box
.bot-bar
  position: fixed
  bottom: 0
  left: 0
  right: 0
  z-index: 2
  background: #fff
  display: flex
  justify-content: space-around
  height: 100rpx
  .b-item
    display: flex
    flex-flow: column
    align-items: center
    justify-content: center
    padding: 5rpx 0 15rpx
    .name
      font-size: 20rpx
      color: #666
    .icon
      background: url("data:image/png;base64,") no-repeat;
      background-size: 600rpx 60rpx
      width: 60rpx
      height: 60rpx
      flex: 0 0 60rpx
    for m in 1 2 3 4 5
      &:nth-child({m})
        .icon
          background-position: -(m - 1)*120rpx 0
        &.active .icon
          background-position: -(2*m - 1)*60rpx 0
        
