      .mzbsi-body
        padding:15rpx 0
        display: flex
        justify-content: space-between
        align-items: center
        font-size: 30rpx
        color: #666
        >view 
          width: 33.333%
          text-align: center
        .mzbis-nr  
          text-align: left
          padding-left: 20rpx
          .mbbl-yuan
            display:inline-block
            vertical-align:middle
            border-radius:50%
            border:2rpx solid #999
            color:#999
            width:40rpx
            height:40rpx
            line-height:40rpx
            text-align:center
            margin-right:10rpx
            font-size: 22rpx
          >span 
            font-size: 35rpx
            color: $theme-color
            display: inline-block
            vertical-align: middle
        .mzbis-nums
          border-right:2rpx solid #eee
      .mzbis-times
        text-align: right
        font-size: 24rpx
        padding:0 15rpx 10rpx
        color:#999
        .mzbis-je
          margin-right: 20rpx
.mainTab
    width: 80rpx
    height: 80rpx
    line-height: 80rpx
    border-radius: 50%
    border: 2rpx solid $theme-color
    background-color: #fff
    color: $theme-color
    text-align: center
    position: fixed
    left: 0
    top: 40%
.rightTips
  padding-left: 10%
  width: 90%
  height: 90rpx
  color: #fff
  background-color: rgba(220,55,84,0.9)
  border-top-left-radius: 120rpx
  border-bottom-left-radius: 120rpx
  position: fixed
  right: -90%
  top: 60%
  animation: rightTipsOne 0.5s
  display: flex
  justify-content: space-between
  align-items: center
  &.suo
    animation: rightTips 0.5s
    right: 0%
  .right-page
    width: 45%
    border-right: 5rpx solid #fff
    &.rightbx
      border-right: none 
      margin-left: 5%
    .rp-nr
      font-size: 20rpx
      span 
        margin-right: 15rpx
      .prn-bigf
        font-size: 25rpx
        font-weight: bold
      .prnb-img
        &.on
          image
            transform: rotate(180deg)
        image
          width: 13rpx
          height: 18rpx
          margin-right: 5rpx
  .rtImg
    position: fixed
    right: 23rpx
    top: 62%
    transition: 0.3s ease
    &.on
      transform: rotate(180deg)
    image
      width: 24rpx
      height: 41rpx
@css{
  @keyframes rightTips{
    0%{  
      right: -90%;
    }
    100%{
      right: 0%;
    }
  }
  @keyframes rightTipsOne{
    0%{
      right: 0%;
    }
    100%{
      right: -90%;
    }
  }  
}
