@import './base'
page
  box-sizing: border-box
  background-color: #eee
.f-swiper-section
  .slide-image
    width 100%
    height 100%
.f-er-menu
  display: flex
  justify-content: space-between
  align-items: center
  padding: 13rpx 10%
  text-align: center
  background-color: #fff
  navigator
    image
      width: 100rpx
      height: 100rpx
p
  display: block
.red
  color: $theme-color
.lv
  color: #80c269
.f-byjg
  border-top:10rpx solid #eee
  display: flex
  justify-content: flex-start
  align-items: center
  padding :10rpx 0 15rpx
  border-bottom:2rpx solid #eee
  line-height: 28rpx
  background-color: #fff
  height: 70rpx
  font-size: 24rpx
  .mb-jzj
    text-align: right
    width: 280rpx
    border-right:2rpx solid #eee
    height: 65rpx 
    line-height: 65rpx
    padding: 0 10rpx
    text-align: left
    span 
      vertical-align: middle
      margin-right: 5rpx
    em
      color: $theme-color
      font-size: 48rpx
      vertical-align: middle
      font-weight: bold
  .mb-byjg
    flex: 1
    display: flex
    justify-content: space-between
    align-items: center
    padding: 0 10rpx
    .mby-box
      min-width: 100rpx
      text-align: center
      .mby-bytitle
        font-size:30rpx
        padding-top: 10rpx
      navigator
        font-size: 20rpx
        color:#999
        padding-top:5rpx
      .mby-small-txt
        font-size: 20rpx
        height: 25rpx
      .mby-big-txt
        font-size:30rpx
        font-weight: bold
        height: 35rpx
        line-height: 35rpx
      .red
        color: $theme-color
      .lv
        color: #80c269
.f-zxbox
    display: flex
    justify-content: flex-start
    align-items: center
    height: 85rpx
    border-bottom:2rpx solid #eee
    background-color: #fff
    padding: 0 10rpx
    .mz-qian
      width: 180rpx
      text-align: right
      .mz-lb-icon
        display: inline-block
        vertical-align: middle
        width: 41rpx
        height: 37rpx
        background: url($picPath + "icon/zixun.png") no-repeat
        background-size: 100%
      span 
        vertical-align: middle
        padding-left:10rpx
        font-size: 30rpx
        font-weight: bold
        color:#666
    .mz-con
      flex:1;
      height: 85rpx
      overflow: hidden
      .mzc-list
        height: 85rpx
        .mzc-list-item
          width: 100%
          color:#666
          display: block
          height: 85rpx
          line-height: 85rpx
          text-overflow: ellipsis
          white-space: nowrap
          overflow: hidden
.l-title-box
  background-color: #fff
  padding: 25rpx 30rpx
  position: relative
  font-size: 30rpx
  color:#666
  display: flex
  justify-content: space-between
  align-items: center
  &:after
    content: ''
    display: block
    width: 14rpx
    height: 24rpx
    position: absolute
    right: 30rpx
    top: 50%
    margin-top: -12rpx
    background: url($picPath + 'icon/page-more.png') no-repeat
    background-position: center
    background-size:cover
  &.l-title-box-no
    &:after
      display: none
  .l-more-link
    padding-right:30rpx;
    font-size:24rpx;
    display:inline-block;
  span 
    border-left: 5rpx solid $theme-color
    padding-left: 15rpx
.f-zy-box
  .mz-page
    padding: 10rpx
    background-color: #eee
    .mzp-list
      height: 390rpx
      .mzp-list-item
        &:last-child
          .mzpl-item
            margin-right 0
        .mzpl-item
          height: 100%
          margin-right:10rpx
          background-color: #fff
          border-radius: 10rpx
          padding: 0 10rpx
          .mzpl-title
            background-color: $theme-color
            color: #fff
            display: inline-block
            padding: 5rpx 10rpx
            border-radius: 4rpx
            margin-top: 10rpx
          .mzpl-jg
            font-size: 23rpx
            text-align: center
            padding: 15rpx 0
            span 
              padding:0 3rpx
          .mzpl-adress
            text-align: center
            color:#666
            font-size: 26rpx
            padding-bottom: 15rpx
            image
              width: 14rpx
              height: 21rpx
              margin-right:5rpx
          .mzpl-nums
            text-align: center
            font-size: 17rpx
          .mzpl-button-page
            padding-top:15rpx
            view 
              display: block
              width: 80%
              margin: 0 auto
              height: 40rpx
              line-height: 40rpx
              background-color: #fff
              color: $theme-color
              text-align: center
              border:2rpx solid $theme-color
              border-radius: 6rpx
              margin-bottom: 15rpx
              font-size: 24rpx
              &.redActive
                background-color: $theme-color
                color:#fff
              &.huiActive
                background-color: #999
                color:#fff
                border:2rpx solid #999
          .mzpl-time
            font-size: 20rpx
            color:#999
            text-align: center
.f-byzy-box
  .mbb-list-page
    background-color: #eee
    padding: 10rpx 10rpx 1rpx
    .mbbl-item
      background-color: #fff
      padding:15rpx 5rpx
      border-radius: 10rpx
      display: flex
      justify-content: space-between
      align-items: center
      text-align: center
      margin-bottom: 15rpx
      >view 
        height: 70rpx
        border-right:2rpx solid #eee
        display: inline-block
        vertical-align: middle
        width: 19%
        font-size: 18rpx
        line-height: 30rpx
        &:first-child
          width: 24%
        &:last-child
          border-right: none 
      .mbbl-one
        font-size: 22rpx
        line-height: 60rpx
        text-align: left
        padding-left:5rpx
      .mbbl-yuan
        display: inline-block
        vertical-align: middle
        border-radius: 50%
        border:2rpx solid #999
        color:#999
        width: 40rpx
        height: 40rpx
        line-height: 40rpx
        text-align: center
        margin-right: 10rpx
      .mbl-middle
        display: inline-block
        vertical-align: middle
        font-size: 18rpx
        &.big
          font-size: 30rpx
      .mbbl-fyp
        border:2rpx solid #999
        border-radius: 6rpx
        padding: 0 10rpx
        display: inline-block
        vertical-align: middle
        color:#999
      .i-zhuanfa
        display: inline-block
        vertical-align: middle
        background:url($picPath + 'icon/zhuanfa.png') no-repeat
        background-size: contain
        width: 29rpx
        height: 20rpx
      .mbbl-zf-txt
        margin-left: 5rpx
      .mbbl-time
        font-size: 20rpx
        color:#999
      .mbbl-btn
        width: 80%
        margin: 0 auto
        border-radius: 6rpx
        padding: 5rpx 0
        color: #ffffff
        background-color: $theme-color
.f-jkzy-box
  .mjb-switch
    switch
      margin-right: 20rpx
    .wx-switch-input
      width:78rpx !important
      height:50rpx !important
      background: $theme-color !important
      border: $theme-color !important
    /*白色样式（false的样式）*/
    .wx-switch-input::before
      background-color: $theme-color !important
      width:78rpx !important
      height: 50rpx !important
    /*红色样式（true的样式）*/
    .wx-switch-input::after
      content: '整单' !important
      font-size: 17rpx
      text-align: center
      line-height: 40rpx
      color: $theme-color
      width: 40rpx !important
      height: 40rpx !important
      margin-top: 5rpx
    &.switchChecked
      .wx-switch-input::after
        content: '子单' !important
  .mjb-list-page
    background-color:#eee
    padding:10rpx 10rpx 1rpx
    .mjb-item
      background-color: #fff
      padding:15rpx 5rpx
      border-radius:10rpx
      display:flex
      justify-content:space-between
      align-items:center
      text-align:center
      margin-bottom:15rpx
      >view 
        height: 70rpx
        line-height: 70rpx
      .mjbi-one
        width 20%
        .mjbi-yuan
          display:inline-block
          vertical-align:middle
          border-radius:50%
          border:2rpx solid #999
          color:#999
          width:40rpx
          height:40rpx
          line-height:40rpx
          text-align:center
          margin-right:10rpx
        >span 
          vertical-align: middle
          color: #333
      .mjbi-two
        width: 58%
        border-left:2rpx solid #eee
        border-right:2rpx solid #eee
        line-height: 20rpx
        color:#666
        .mjbi-head-jf
          display: flex
          justify-content: space-between
          align-items: center
          border-bottom:2rpx solid #eee
          height: 30rpx
          line-height: 30rpx
          font-size:20rpx
          padding-bottom: 5rpx
          >span
            display: block
            border-right: 2rpx solid #eee
            text-align: center
            flex:1
            &:last-child
              border-right none 
          .mhbi-em
            border:2rpx solid #999
            border-radius: 6rpx
            width: 75%
            margin: 0 auto
            display: block
            height: 28rpx
            line-height: 28rpx
        .mjbi-txt-yc
          overflow: hidden
          text-overflow: ellipsis
          white-space: nowrap
          height: 35rpx
          line-height: 35rpx
          font-size: 20rpx
      .mjbi-three
        width: 20%
        line-height: 30rpx
        font-size: 20rpx
        color:#999
        .mbbl-btn
          width: 80%
          margin: 0 auto
          border-radius: 6rpx
          padding: 5rpx 0
          height: 30rpx
          line-height: 30rpx
          font-size: 18rpx
          color: #ffffff
          background-color: $theme-color
.f-zxcjxx-box
  .mzb-section
    padding: 10rpx 10rpx 1rpx
    background-color: #eee
    .mzbs-item
      background-color: #fff
      margin-bottom: 15rpx
      border-radius: 6rpx
      .mzbsi-title
        padding: 10rpx 20rpx 10rpx 30rpx
        display: flex
        justify-content: space-between
        align-items: center
        .mzbsi-jktitle
