page
  padding-top: 85rpx
  box-sizing: border-box
.header-wrap
  position: fixed
  top: 0
  left: 0
  right: 0
  z-index: 2
  height: 85rpx
  background: #d7092d
  display: flex
  align-items: center
  justify-content: space-between
  .h-back
    position: relative
    z-index: 2
    background: url("data:image/png;base64,") no-repeat;
    background-size: contain
    width: 24rpx
    height: 41rpx
    margin-left: 30rpx
  .h-title
    position: absolute
    left: 0
    right: 0
    top: 0
    line-height: 85rpx
    color: #fff
    font-size: 30rpx
    text-align: center