@import './base'
page
  padding-top: 85rpx
  box-sizing: border-box
.header-wrap
  position: fixed
  top: 0
  left: 0
  right: 0
  z-index: 2
  height: 85rpx
  background: #d7092d
  display: flex
  align-items: center
  justify-content: space-between
  .h-back
    position: relative
    z-index: 2
    background: url("data:image/png;base64,") no-repeat;
    background-size: contain
    width: 24rpx
    height: 41rpx
    margin-left: 30rpx
  .h-title
    position: absolute
    left: 0
    right: 0
    top: 0
    line-height: 85rpx
    color: #fff
    font-size: 30rpx
    text-align: center
  .h-right
    position: absolute
    z-index: 2
    margin-right: 30rpx
    color: #fff
    font-size: 28rpx
    right:0
    top:0
    line-height:85rpx
    height:85rpx
    .navigator-hover
      background: none 
      opacity: 1
    .h-editbx
      color: #ffffff
      font-size: 24rpx
      .h-editi
        margin-left: 5rpx
        display: inline-block
        width: 23rpx
        height: 26rpx
        background:url("data:image/png;base64,")  no-repeat
        background-size:contain
      .fdj
        display: inline-block;
        vertical-align: middle;
        width: 35rpx;
        height: 42rpx;
        background:url("data:image/png;base64,")  no-repeat;
        background-size:contain;
  .h-searchbx
    flex:1
    background-color: #c10828
    border-radius: 40rpx
    font-size: 24rpx
    padding: 0 20rpx
    display: flex
    justify-content: flex-start
    align-items: center
    height: 60rpx
    margin:0 40rpx
    .h-searchi
      display: inline-block;
      vertical-align: middle;
      width: 35rpx;
      height: 42rpx;
      background:url("data:image/png;base64,")  no-repeat;
      background-size:contain;
    .h-searchinp
      flex:1
      height 50rpx
      line-height: 50rpx
      padding: 5rpx 20rpx
    .placeholderStyle
      color: #eeeeee
